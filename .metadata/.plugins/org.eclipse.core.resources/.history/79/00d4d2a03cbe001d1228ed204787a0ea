package divisorMultiple;

import java.util.Scanner;

public class Test {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			System.out.println("시행할 작업을 선택하시오.(1: 약수, 2: 최소공배수, 3: 최대공약수, 0: 종료)");
			int x = sc.nextInt();
			
			switch(x) {
			case 1:
				divisor(x);
			case 2:
			case 3:
			default:
				System.out.println("0~3까지의 숫자만 입력하시오");
			}
		}
	}
}
