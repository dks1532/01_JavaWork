public class Game {
	public void game() {
		Dice d = new Dice();
		Scanner sc = new Scanner(System.in);
		
		System.out.println("1번째 플레이어의 주사위를 굴립니다. 계속하려면 엔터를 입력하세요");
		sc.nextLine();
		int p1 = d.roll();
		
		System.out.println("2번째 플레이어의 주사위를 굴립니다. 계속하려면 엔터를 입력하세요");
	}
}
